    "emotion-server": "^10.0.7",
    "facepaint": "^1.2.1",
    "gatsby": "2.0.118",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-catch-links": "^2.0.10",
    "gatsby-plugin-emotion": "4.0.1",
    "gatsby-plugin-manifest": "2.0.17",
    "gatsby-plugin-material-ui": "^1.2.4",
    "gatsby-plugin-offline": "2.0.23",
